            label = "TAP_DANCE_1";
            #binding-cells = <0>;
            tapping-term-ms = <400>;
            bindings = <&kp Q>, <&none>, <&bt BT_SEL 1>;
        };
        //QW|BT_SEL2
        td2: tap_dance_2 {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_2";
            #binding-cells = <0>;
            tapping-term-ms = <400>;
            bindings = <&kp W>, <&none>, <&bt BT_SEL 2>;
        };
        //E|BT_SEL3
