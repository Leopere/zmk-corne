    //&bootloader &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3 &bt BT_SEL 4 &bt BT_CLR
    behaviors {
        //ESC|bootloader
        td0: tap_dance_0 {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_0";
            #binding-cells = <0>;
            tapping-term-ms = <400>;
            bindings = <&kp ESC>, <&none>, <&none>, <&none>, <&none>, <&none>, <&none>, <&none>, <&none>, <&bootloader>;
        };
        //Q|BT_SEL1
        td1: tap_dance_1 {
