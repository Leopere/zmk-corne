        //T|BT_CLR
        td5: tap_dance_5 {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_5";
            #binding-cells = <0>;
            tapping-term-ms = <400>;
            bindings = <&kp T>, <&none>, <&bt BT_CLR>;
        };
    };
    keymap {
        compatible = "zmk,keymap";
        // Row1
        // F11, q,w,e,r,t
        // Row2
        // 8,a,s,d,f,g
        //Row3
        //7,z,x,c,v,b
        //Row4
        //f10, F7, F9
        default_layer {
            bindings = <
            //Row1
                &td0        &td1        &td2        &td3        &td4        &td5
            //Row2
                &kp TAB     &kp A       &kp S       &kp D       &kp F       &kp G
            //Row3
                &kp LSHIFT   &kp Z       &kp X       &kp C       &kp V       &kp B
            //Row4
                                                                &kp LCMD       &kp SPACE   &kp RSHFT
            >;
        };
