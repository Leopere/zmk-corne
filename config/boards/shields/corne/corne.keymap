#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
/ {
    keymap {
        compatible = "zmk,keymap";

        // Row1
        // F11, q,w,e,r,t
        // Row2
        // 8,a,s,d,f,g
        //Row3
        //7,z,x,c,v,b
        //Row4
        //f10, F7, F9
        default_layer {
            bindings = <
                &kp ESC     &kp Q       &kp W       &kp E       &kp R       &kp T
                &kp TAB     &kp A       &kp S       &kp D       &kp F       &kp G
                &kp LCTRL   &kp Z       &kp X       &kp C       &kp V       &kp B
                &mo 1    &kp LSHFT   &kp SPACE
            >;
        };
        // Row1
        // F11, q,w,e,r,t
        // Row2
        // 8,a,s,d,f,g
        //Row3
        //7,z,x,c,v,b
        //Row4
        //f10, F7, F9
        bt_layer {
            bindings = <
                &bootloader &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3 &bt BT_SEL 4 &bt BT_CLR
                &none     &none       &none       &none       &none       &none
                &none     &none       &none       &none       &none       &none
                &none     &none       &none
            >;
        };
    };
};